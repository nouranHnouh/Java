
public class SalesCalculator {
	
	private int totalSalesOneWeek;
	private int sales; 
	
	public SalesCalculator() {
		
		this.totalSalesOneWeek=0;
	}
	
	
	public void setValue(int value) {
		this.totalSalesOneWeek+=value;
	}
	
	public int getValue() {
		return totalSalesOneWeek; 
	}
	
	public 
	
	public double calcSalesEarning() {
		double earningSalary;
		earningSalary=(double) (200+(9/100)*getValue()); 
		return earningSalary; 
		
	}

}
